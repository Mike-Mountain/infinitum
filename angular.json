            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/simple-todo/src/favicon.ico",
              "apps/simple-todo/src/assets"
            ],
            "styles": ["apps/simple-todo/src/styles.scss"],
            "scripts": [],
            "extraWebpackConfig": "apps/simple-todo/webpack.config.js",
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/simple-todo/src/environments/environment.ts",
                  "with": "apps/simple-todo/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "apps/simple-todo/webpack.prod.config.js"
            },
            "dev": {
              "fileReplacements": [
                {
                  "replace": "apps/simple-todo/src/environments/environment.ts",
                  "with": "apps/simple-todo/src/environments/environment.dev.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "simple-todo:build:production",
              "extraWebpackConfig": "apps/simple-todo/webpack.prod.config.js"
            },
            "dev": {
              "browserTarget": "simple-todo:build:dev"
            },
            "development": {
              "browserTarget": "simple-todo:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "extraWebpackConfig": "apps/simple-todo/webpack.config.js",
            "port": 4200
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "simple-todo:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/simple-todo/src/**/*.ts",
              "apps/simple-todo/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "outputs": ["coverage/apps/simple-todo"],
          "options": {
            "jestConfig": "apps/simple-todo/jest.config.js",
            "passWithNoTests": true,
            "extraWebpackConfig": "apps/simple-todo/webpack.config.js"
          }
        }
      }
